{
    "name": "agoalofalife/cart",
    "description": "",
    "type": "library",
    "keywords": [
        "cart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "agoalofalife",
            "email": "agoalofalife@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/container": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/events": "^5.4",
        "illuminate/config": "^5.4",
        "symfony/finder": "^3.3",
        "symfony/console": "^3.3",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "larapack/dd": "^1.1",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.1",
        "symfony/var-dumper":"^3.3.5"
    },
    "autoload": {
        "psr-4": {
            "Cart\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cart\\Tests\\": "tests/"
        }
    },
    "bin": [
        "cart"
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
